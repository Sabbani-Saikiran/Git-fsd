package com.app.TDD.demo;

import org.testng.Assert;

public class CalculatorTest {
	
	Calculator calculator = new Calculator();
	
	public void testadd() {
		
		int a = 5;
		int b = 7;
		
		int result = calculator.add(a,b);
		
		//Assert
		
		Assert.assertEquals(result, 12, "Addition result is incorrect");
	}

}
